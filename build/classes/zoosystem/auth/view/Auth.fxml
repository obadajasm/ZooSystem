<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="zoosystem.auth.controllers.AuthController">
   <children>
             <Button id="loginBtn" fx:id="loginBtn" layoutX="239.0" layoutY="300" mnemonicParsing="false" onAction="#loginBtnClck" prefHeight="25.0" prefWidth="150.0" text="login" />

      <ImageView fitHeight="197.0" fitWidth="400.0" layoutX="100.0" pickOnBounds="true">
         <image>
            <Image url="@../../assets/images/logo.png" />
         </image>
      </ImageView>
       <TextField fx:id="userNameTF" layoutX="239.0" layoutY="150" promptText="User Name" />
      <TextField fx:id="emailTF" layoutX="239.0" layoutY="200" promptText="Email" />
       <PasswordField fx:id="passwordTF" layoutX="239.0" layoutY="250" promptText="Password" />

      <Button fx:id="signupBtn" cacheShape="false" centerShape="false" depthTest="DISABLE" focusTraversable="false" layoutX="239.0" layoutY="350" mnemonicParsing="false" onAction="#signupClck" scaleShape="false" text="signup ?">
         <opaqueInsets>
            <Insets />
         </opaqueInsets></Button>
        
   </children>
</AnchorPane>
